#!/bin/bash
#SBATCH --job-name="MPI output"
#SBATCH --ntasks=8
#SBATCH --cpus-per-task=1
#SBATCH --mem-per-cpu=8G
#SBATCH --time=20:00
#SBATCH --output=out/mpi_out.out

module load OpenMPI
module load Info0939Tools

mpicc MPI/fdtd_mpi.c -o bin/fdtd_mpi -lm -O3 
cd ./example_inputs/simple3d
srun ../../bin/fdtd_mpi param_3d.txt
rm cutx.msh
data2gmsh -cutx 0.5 out_p.dat 
cd ../..